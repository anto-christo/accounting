{
  "name": "frappe-accounting",
  "description": "Simple Accounting app for everyone",
  "productName": "Frappe-Accounting",
  "version": "0.0.2",
  "author": {
    "name": "Frappe Technologies Pvt. Ltd.",
    "email": "hello@frappe.io"
  },
  "build": {
    "asarUnpack": [
      "node_modules/sharp"
    ],
    "productName": "Frappe-Accounting",
    "appId": "io.frappe.accounting",
    "linux": {
      "target": "deb"
    },
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron"
    ]
  },
  "main": "dist/electron/main.js",
  "license": "MIT",
  "scripts": {
    "test": "mocha tests",
    "start": "frappe start",
    "electron": "cross-env ELECTRON=true frappe start electron",
    "pack": "cross-env NODE_ENV=production ELECTRON=true frappe build electron",
    "dist": "yarn pack && electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "frappejs": "github:anto-christo/frappejs#patches",
    "popper.js": "^1.14.4",
    "vue-color": "^2.7.0",
    "vue-toasted": "^1.1.25"
  },
  "devDependencies": {
    "electron-builder": "^20.38.4"
  }
}
